<h1> Signup Now to Begin Browsing:</h1>
<%= form_for @user do |f| %>
<%= f.label :username %>
<%= f.text_field :username %>
<br>
<%= f.label :password %>
<%= f.password_field :password_digest %>
<br>
<%=f.label :password_confirmation %>
<%=f.password_field :password_confirmation%>

<br>
<%= f.submit %>
<% end %>